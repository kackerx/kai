{
    // "gopls": {
    //     "build.flags": [
    //         "-tags=wireinject"
    //     ]
    // },
    // "go.buildTags": "wireinject", 
    "go.toolsManagement.autoUpdate": true,
    "go.useLanguageServer": true,
    "go.lintOnSave": "package",
    "go.vetOnSave": "package",
    "go.formatTool": "goimports",
    "go.lintTool": "golangci-lint",
    "go.buildOnSave": "off",
    "go.testOnSave": false,
    "go.coverOnSave": false,
    "go.gocodeAutoBuild": false,
    "go.inferGopath": false,
    "go.gotoSymbol.includeImports": true,
    "go.gotoSymbol.includeGoroot": true,
    "go.autocompleteUnimportedPackages": true,
    "go.docsTool": "godoc",
    "go.alternateTools": {
        "go": "go"
    },
    "go.testFlags": [
        "-v",
        "-race"
    ],
    "go.testTimeout": "30s",
    "go.coverMode": "atomic",
    "files.exclude": {
        "**/data/*.db": true,
        "**/data/*.log": true,
        "**/.DS_Store": true
    },
    "files.watcherExclude": {
        "**/data/**": true,
        "**/vendor/**": true,
        "**/node_modules/**": true
    },
    "search.exclude": {
        "**/vendor": true,
        "**/node_modules": true,
        "**/data": true
    },
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "debugConsoleBeautify.enabled": true,
    "debugConsoleBeautify.formatJSON": true,
    "debugConsoleBeautify.theme": "dark",
    "debugConsoleBeautify.rules": [
        {
            "pattern": "error|ERROR|Error",
            "color": {
                "dark": "#ff6464",
                "light": "#ff6464"
            },
            "bold": true
        },
        {
            "pattern": "INFO*SELECT",
            "color": {
                "dark": "#ff0000",
                "light": "#ff0000"
            }
        },
        {
            "pattern": "time",
            "color": {
                "dark": "#ff0000",
                "light": "#ff0000"
            }
        },
        {
            "pattern": "info|INFO|Info",
            "color": {
                "dark": "#6BB6FF",
                "light": "#0066CC"
            }
        },
        {
            "pattern": "warn|WARN|Warn",
            "color": {
                "dark": "#ff6464",
                "light": "#ff6464"
            }
        },
        {
            "pattern": "panic|PANIC|Panic",
            "color": {
                "dark": "#FF5555",
                "light": "#CC0000"
            }
        },
        {
            "pattern": "GIN-debug",
            "color": {
                "dark": "#eeff05",
                "light": "#eeff05"
            }
        },
        {
            "pattern": "GIN",
            "color": {
                "dark": "#ff0000",
                "light": "#ff0000"
            }
        },
        {
            "pattern": "*SELECT.*?FROM.*?WHERE.*?$",
            "color": {
                "dark": "#ff0000",
                "light": "#ff0000"
            }
        }
    ]
}